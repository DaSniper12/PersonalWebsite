{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/synesthesia/**",
        "destination": "/synesthesia/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}